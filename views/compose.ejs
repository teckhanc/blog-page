<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle" value="" id="postTitle">
  </div>
  <div class="form-group">
    <label for="postContent">Post</label>
    <textarea class="form-control" name="postContent" value="" id="postContent" rows="7"></textarea>
  </div>
  <div class="form-group">
    <button type="Submit" name="button" class="btn btn-primary">Publish</button>
  </div>
</form>

<%- include("partials/footer") -%>
